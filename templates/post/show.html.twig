{% extends "base.html.twig" %}

{% block title %}Post{% endblock %}

{% block body %}
    <h1>{{ post.title }}</h1>

    <div>
        {{ post.content }}
    </div>

    <hr>

    <a href="{{ path('post_edit', { id: post.id }) }}">Edit</a>
    <a href="{{ path('post_delete', { id: post.id }) }}">Delete</a>
{% endblock %}